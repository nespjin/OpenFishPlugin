<?xml version="1.0" encoding="UTF-8" ?>

<manifest xmlns="http://schema.nesp.com/ppk/res/fishplugin" pluginId="">
    <plugin name="插件名称"
            minSdkVersion="1"
            targetSdkVersion="1"
            introduction="[CDATA! ]"
            type="movie"
            deviceType="all">

        <parent path="./parent.xml"/>

        <tags>
            <tag name="das"/>
        </tags>

        <ref>
            <variable name="categoryJs">
                <value>
                    ./js/category.js
                </value>
            </variable>
            <variable name="dsl">
                <value>
                    <Dsl>

                    </Dsl>
                </value>
            </variable>
        </ref>

        <pages>
            <!--Support for Desktop-->
            <homePage deviceType="desktop" url="http://www.xxx.com/pc">
                <js/>
            </homePage>

            <homePage url="http://www.xxx.com/home">
                <js path="./js/home.js"/>
            </homePage>

            <categoryPage category="movie" url="http://www.xxx.com/category/movie">
                <js path="{{categoryJs}}"/>
            </categoryPage>

            <categoryPage category="soap" url="http://www.xxx.com/category/soap">
                <js path="{{categoryJs}}"/>
            </categoryPage>

            <categoryPage category="variety" url="http://www.xxx.com/category/variety">
                <js path="{{categoryJs}}"/>
            </categoryPage>

            <categoryPage category="anim" url="http://www.xxx.com/category/anim">
                <js path="{{categoryJs}}"/>
            </categoryPage>

            <searchPage url="http://www.xxx.com/search">
                <js path="./js/search.js"/>
            </searchPage>

            <detailPage url="http://www.xxx.com/detail">
                <js path="./js/detail.js"/>
            </detailPage>
        </pages>

    </plugin>
</manifest>